<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="entities.ReservationsEntity" table="reservations">
        <id name="idReservation" column="ID_reservation">
            <generator class="increment"/>
        </id>
        <property name="idUser" column="ID_user" />
        <property name="idRoom" column="ID_room" />
        <property name="meetTimeStart" type="timestamp" column="meet_time_start" />
        <property name="meetTimeEnd" type="timestamp" column="meet_time_end" />

    </class>
</hibernate-mapping>
